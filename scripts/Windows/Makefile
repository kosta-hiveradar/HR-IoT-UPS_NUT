	$(AM_V_CCLD)$(LINK) $(halt_OBJECTS) $(halt_LDADD) $(LIBS)

nut$(EXEEXT): $(nut_OBJECTS) $(nut_DEPENDENCIES) $(EXTRA_nut_DEPENDENCIES) 
	@rm -f nut$(EXEEXT)
	$(AM_V_CCLD)$(LINK) $(nut_OBJECTS) $(nut_LDADD) $(LIBS)

